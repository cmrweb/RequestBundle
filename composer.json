{
    "name": "cmrweb/request-bundle",
    "description": "Symfony request hepler bundle",
    "type": "symfony-bundle",
    "require": {
        "symfony/flex": "^2.10",
        "symfony/http-client": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.3",
        "symfony/property-access": "^8.0",
        "symfony/property-info": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cmrweb\\RequestBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "cmrweb",
            "email": "tykrastagames@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
